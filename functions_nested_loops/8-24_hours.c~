#include "main.h"
#include <time.h>
/**
*jack_bauer- Moneda 3 de C
*: Variable desconocida
*Return: 0
*/

void jack_bauer(void)
{
  for (int hour = 0; hour < 24; hour++)
    {
      for (int minute = 0; minute < 60; minute++)
	{
	  _putchar("%02d:%02d\", hour, minute);
		   }
	}
    }

 int main(void)
 {
   jack_bauer();
   return 0;
 }
