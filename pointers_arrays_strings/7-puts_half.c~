#include "main.h"

/**
 *puts_half- Moneda 13
 *@str:variable
 *Return:lenght
*/

void puts_half(char *str)
{
   int len = strlen(str);
    int half_len = len / 2;

    if (len % 2 != 0)
      {
        half_len++;
    }
        for (int i = half_len; i < len; i++)
	  {
        _putchar(str[i]);
    }
    _putchar('\n');
}
